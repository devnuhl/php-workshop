{
    "name": "php-workshop/php-workshop",
    "description": "PHP Workshop Application",
    "license": "MIT",
    "authors": [
        {
            "name": "Aydin Hassan",
            "email": "aydin@hotmail.co.uk"
        },
        {
            "name": "Michael Woodward",
            "email": "mikeymike.mw@gmail.com"
        }
    ],
    "require" : {
        "php" : ">=5.6",
        "php-di/php-di": "^5.0",
        "symfony/process": "^2.7",
        "symfony/filesystem": "^2.7",
        "fzaninotto/faker": "^1.5",
        "aydin-hassan/cli-md-renderer": "~1.0",
        "php-school/psx": "~1.0",
        "mikeymike/cli-menu": "dev-master",
        "nikic/php-parser": "dev-master#98d28d7aa04e2d92fea2b375373878d0b1fce992"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload" : {
        "psr-4" : {
            "PhpWorkshop\\PhpWorkshop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "PhpWorkShop\\PhpWorkShopTest\\": "test/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:AydinHassan/cli-menu.git"
        }
    ],
    "bin": ["bin/php-workshop"]
}
